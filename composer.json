{
    "name": "zaxxas/laravel-simple-chat-notification",
    "description": "This package is implemented for notifying to specified chat tools (slack, .etc)",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zaxxas\\NotifyToChatTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zaxxas\\NotifyToChatTools\\": "src/",
            "Illuminate\\Tests\\Notifications\\": "tests/"
        }
    },
    "authors": [
    {
        "name": "zaxxas",
        "email": "zaxxxxxas@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4.0",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/collections": "^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.1",
        "orchestra/testbench": "^8.0"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --testsuite test"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zaxxas\\NotifyToChatTools\\Providers\\NotificationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}