{
    "name": "zaxxas/notify-to-chat-tools",
    "description": "This package is implemented for notifying to specified chat tools (slack, .etc)",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zaxxas\\NotifyToChatTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zaxxas\\NotifyToChatTools\\": "src/",
            "Illuminate\\Tests\\Notifications\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "zaxxas",
            "email": "zaxxxxxas@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/support": "^9.0",
        "illuminate/collections": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "dev-master"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --testsuite test"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zaxxas\\NotifyToChatTools\\Providers\\NotificationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
